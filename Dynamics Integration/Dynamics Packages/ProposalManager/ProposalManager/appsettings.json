{
  "WebHooks:DynamicsCrm:SecretKey": {
    "opportunity": "<opportunitiesSecret>",
    "connection": "<connectionsSecret>"
  },
  "Dynamics365": {
    "OrganizationUri": "<organizationApiUri>/",
    "ProposalManagerCategoryId": 741530000,
    "RootDrive": "<driveName>",
    "OpportunityMapping": {
      "MetadataFields": [
        {
          "From": "description",
          "To": "Notes"
        },
        {
          "From": "createdon",
          "To": "Opened Date"
        },
        {
          "From": "budgetamount",
          "To": "Deal Size"
        }
      ],
      "Status": [
        {
          "From": 1,
          "To": 2
        },
        {
          "From": 2,
          "To": 7
        },
        {
          "From": 3,
          "To": 10
        }
      ]
    }
  },
  "OneDrive": {
    "WebhookSecret": "<oneDriveWebhookSecret>",
    "FormalProposalCallbackRelativeUrl": "/api/dynamics/FormalProposal",
    "AttachmentCallbackRelativeUrl": "/api/dynamics/Attachment"
  }
}